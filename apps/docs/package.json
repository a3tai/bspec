{
  "name": "docs",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "bunx --bun svelte-kit sync",
    "check": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bunx --bun svelte-kit sync && bunx --bun svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "bunx prettier --write .",
    "lint": "bunx prettier --check . && bunx eslint .",
    "build:data": "bun scripts/build-data.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.22.0",
    "@sveltejs/adapter-cloudflare": "^7.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "mdsvex": "^0.12.3",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "js-untar": "^2.0.0",
    "pako": "^2.1.0",
    "tar": "^7.5.1"
  },
  "dependencies": {
    "framer-motion": "^12.23.22",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "marked": "^16.3.0",
    "marked-highlight": "^2.2.2",
    "svelte-motion": "^0.12.2"
  }
}
